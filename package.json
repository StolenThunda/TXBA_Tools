{
  "name": "txba-tools",
  "version": "1.0.0",
  "description": "A quasar version of Proplayer's tools. one codebase for all!",
  "productName": "TXBA Tools",
  "author": "Tony Moses <tonym415@gmail.com>",
  "private": true,
  "scripts": {
    "build": "npx quasar b --debug",
    "build:deploy": "yarn run build && yarn run propogate",
    "build:android": "npx quasar build -m capacitor -T android --debug --ide",
    "build:ios": "npx quasar build -m capacitor -T ios --debug --ide",
    "build:electron": "npx quasar build -m electron --debug ",
    "build:electron:interactive": "npx epi",
    "dev": " npx quasar d",
    "dev:cordova:android" : "npx quasar d -m cordova -T android --debug  --ide",
    "dev:cordova:id" : "npx quasar d -m cordova -T ios --debug  --ide",
    "dev:capacitor:android" : "npx quasar d -m capacitor -T android --debug  --ide",
    "dev:electron": "unset NODE_OPTIONS && npx quasar dev -m electron -- --no-sandbox --disable-setuid-sandbox",
    "dev:ios": "npx quasar dev -m capacitor -T ios --ide",
    "lint": "eslint --ext .js,.vue ./",
    "propogate": "cp --update --recursive --verbose --force dist/spa/* ../txba-demo",
    "start": "quasar d"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.1",
    "@capacitor/core": "^2.4.1",
    "@electron/remote": "^2.0.5",
    "@quasar/cli": "^1.2.2",
    "@quasar/extras": "^1.12.4",
    "@quasar/icongenie": "^2.4.2",
    "@svgdotjs/svg.filter.js": "^3.0.8",
    "@svgdotjs/svg.js": "^3.1.2",
    "@svgdotjs/svg.panzoom.js": "^2.1.2",
    "aubiojs": "^0.0.1",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.3",
    "cordova": "^11.0.0",
    "core-js": "^3.6.5",
    "electron-packager-interactive": "^0.1.3",
    "fastclick": "^1.0.6",
    "firebase": "^8.2.6",
    "global": "^4.4.0",
    "howler": "^2.2.3",
    "quasar": "^1.18.2",
    "webpack": "^5.20.2",
    "wine": "^0.9.13"
  },
  "devDependencies": {
    "@quasar/app": "^2.2.1",
    "babel-eslint": "^10.0.1",
    "electron": "^13.0.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.1.2"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
